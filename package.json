{
  "name": "psad_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "./node_modules/react-native-maps/enable-google-maps 'ios/**'",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "format-json": "^1.0.3",
    "formik": "^1.4.3",
    "libphonenumber-js": "^1.7.8",
    "lodash": "^4.17.11",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.23.0",
    "native-base": "^2.8.1",
    "prop-types": "^15.6.2",
    "querystringify": "^2.1.0",
    "ramda": "^0.25.0",
    "react": "^16.8.2",
    "react-native": "^0.58.6",
    "react-native-base64": "0.0.2",
    "react-native-calendars": "^1.21.0",
    "react-native-checkbox": "^2.0.0",
    "react-native-config": "^0.11.7",
    "react-native-dash": "0.0.9",
    "react-native-drawer": "^2.5.1",
    "react-native-extended-stylesheet": "^0.10.0",
    "react-native-flexi-radio-button": "^0.2.2",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-google-places-autocomplete": "^1.3.9",
    "react-native-i18n": "^2.0.15",
    "react-native-indicators": "^0.13.0",
    "react-native-keyboard-avoiding-view": "^1.0.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-maps": "^0.23.0",
    "react-native-modal": "^7.0.2",
    "react-native-progress": "^3.5.0",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-splash-screen": "^3.1.1",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "^3.0.7",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2",
    "reduxsauce": "^1.0.0",
    "seamless-immutable": "^7.1.4",
    "validate.js": "^0.12.0",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.2.3",
    "@storybook/react-native": "^3.2.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "enzyme": "^2.6.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^0.13.1",
    "ignite-i18n": "^1.1.1",
    "ignite-native-base-boilerplate": "^2.0.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.3",
    "mockery": "^2.0.0",
    "react-addons-test-utils": "~15.4.1",
    "react-dom": "16.0.0-alpha.12",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "reactotron-react-native": "^1.12.0",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "schedule": "^0.4.0",
    "snazzy": "^8.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "rnpm": {
    "assets": [
      "App/Assets/Fonts/OpenSans",
      "./assets/fonts/"
    ]
  }
}
